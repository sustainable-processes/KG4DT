     xmlns:system="https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/system.owl#"
     xmlns:fundamental_concepts="https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#"
