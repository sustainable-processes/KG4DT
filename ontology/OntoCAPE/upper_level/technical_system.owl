    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/system.owl#hasAspectSystem -->

    <owl:ObjectProperty rdf:about="&system;hasAspectSystem"/>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/system.owl#isConsideredUnderAspectOf -->

    <owl:ObjectProperty rdf:about="&system;isConsideredUnderAspectOf"/>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/system.owl#isDirectlyRelatedTo -->

    <owl:ObjectProperty rdf:about="&system;isDirectlyRelatedTo"/>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/system.owl#representsAspectOf -->

    <owl:ObjectProperty rdf:about="&system;representsAspectOf"/>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/technical_system.owl#constrains -->

    <owl:ObjectProperty rdf:about="#constrains">
        <rdfs:comment rdf:datatype="&xsd;string"
            >The constrains relation indicates that a SystemRealization imposes constraints on the SystemBehavior.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&system;isDirectlyRelatedTo"/>
        <rdfs:range rdf:resource="#SystemBehavior"/>
        <rdfs:domain rdf:resource="#SystemRealization"/>
        <owl:inverseOf rdf:resource="#isConstrainedBy"/>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/technical_system.owl#evaluates -->

    <owl:ObjectProperty rdf:about="#evaluates">
        <rdfs:comment rdf:datatype="&xsd;string"
            >The relation Evaluates refers from a performance measure to the AspectSystem the performance of which is evaluated.</rdfs:comment>
        <rdfs:range rdf:resource="&system;AspectSystem"/>
        <rdfs:subPropertyOf rdf:resource="&system;isDirectlyRelatedTo"/>
        <rdfs:domain rdf:resource="#SystemPerformance"/>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/technical_system.owl#fulfills -->

    <owl:ObjectProperty rdf:about="#fulfills">
        <rdfs:comment rdf:datatype="&xsd;string"
            >The Fulfills relation states that a SystemFunction fulfills a particular SystemRequirement.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&system;isDirectlyRelatedTo"/>
        <rdfs:domain rdf:resource="#SystemFunction"/>
        <rdfs:range rdf:resource="#SystemRequirements"/>
        <owl:inverseOf rdf:resource="#isAchievedThrough"/>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/technical_system.owl#hasBehavioralAspect -->

    <owl:ObjectProperty rdf:about="#hasBehavioralAspect">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >The relation points to the behavioral aspect of a TechnicalSystem.</rdfs:comment>
        <rdfs:domain rdf:resource="&system;System"/>
        <rdfs:subPropertyOf rdf:resource="&system;hasAspectSystem"/>
        <rdfs:range rdf:resource="#SystemBehavior"/>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/technical_system.owl#hasFunctionalAspect -->

    <owl:ObjectProperty rdf:about="#hasFunctionalAspect">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >The relation points to the functional aspect of a TechnicalSystem.</rdfs:comment>
        <rdfs:domain rdf:resource="&system;System"/>
        <rdfs:subPropertyOf rdf:resource="&system;hasAspectSystem"/>
        <rdfs:range rdf:resource="#SystemFunction"/>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/technical_system.owl#hasPerformanceAspect -->

    <owl:ObjectProperty rdf:about="#hasPerformanceAspect">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >The relation points to the performance aspect of a TechnicalSystem.</rdfs:comment>
        <rdfs:domain rdf:resource="&system;System"/>
        <rdfs:subPropertyOf rdf:resource="&system;hasAspectSystem"/>
        <rdfs:range rdf:resource="#SystemPerformance"/>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/technical_system.owl#hasPerformanceMeasure -->

    <owl:ObjectProperty rdf:about="#hasPerformanceMeasure">
        <rdfs:comment rdf:datatype="&xsd;string"
            >The relation hasPerformance points from an AspectSystem, the performance of which is evaluated, to the performance measure.</rdfs:comment>
        <rdfs:domain rdf:resource="&system;AspectSystem"/>
        <rdfs:subPropertyOf rdf:resource="&system;isDirectlyRelatedTo"/>
        <rdfs:range rdf:resource="#SystemPerformance"/>
        <owl:inverseOf rdf:resource="#evaluates"/>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/technical_system.owl#hasPhenomenon -->

    <owl:ObjectProperty rdf:about="#hasPhenomenon">
        <rdfs:comment rdf:datatype="&xsd;string"
            >The relation hasPhenomenon assings a Phenomenon to a SystemBehavior.</rdfs:comment>
        <rdfs:range rdf:resource="#Phenomenon"/>
        <rdfs:domain rdf:resource="#SystemBehavior"/>
        <rdfs:subPropertyOf rdf:resource="&fundamental_concepts;object-featureRelation"/>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/technical_system.owl#hasRealizationAspect -->

    <owl:ObjectProperty rdf:about="#hasRealizationAspect">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >The relation points to the realization aspect of a TechnicalSystem.</rdfs:comment>
        <rdfs:domain rdf:resource="&system;System"/>
        <rdfs:subPropertyOf rdf:resource="&system;hasAspectSystem"/>
        <rdfs:range rdf:resource="#SystemRealization"/>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/technical_system.owl#hasRequirementsAspect -->

    <owl:ObjectProperty rdf:about="#hasRequirementsAspect">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >The relation points to the requirements aspect of a TechnicalSystem.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&system;hasAspectSystem"/>
        <rdfs:range rdf:resource="#SystemRequirements"/>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/technical_system.owl#isAchievedThrough -->

    <owl:ObjectProperty rdf:about="#isAchievedThrough">
        <rdfs:comment rdf:datatype="&xsd;string"
            >The relation isAchievedThrough states that a SystemRequirement can be achieved by means of a some SystemFunction.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&system;isDirectlyRelatedTo"/>
        <rdfs:range rdf:resource="#SystemFunction"/>
        <rdfs:domain rdf:resource="#SystemRequirements"/>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/technical_system.owl#isConstrainedBy -->

    <owl:ObjectProperty rdf:about="#isConstrainedBy">
        <rdfs:comment rdf:datatype="&xsd;string"
            >The constrains relation states that the SystemBehavior is limited by the constraints imposed by the SystemRealization.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&system;isDirectlyRelatedTo"/>
        <rdfs:domain rdf:resource="#SystemBehavior"/>
        <rdfs:range rdf:resource="#SystemRealization"/>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/technical_system.owl#isInfluencedBy -->

    <owl:ObjectProperty rdf:about="#isInfluencedBy">
        <rdfs:comment rdf:datatype="&xsd;string"
            >The relation indicates which Properties are influenced by a particular Phenomenon.</rdfs:comment>
        <rdfs:domain rdf:resource="&system;Property"/>
        <rdfs:range rdf:resource="#Phenomenon"/>
        <rdfs:subPropertyOf rdf:resource="&fundamental_concepts;object-featureRelation"/>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/technical_system.owl#isRealizedBy -->

    <owl:ObjectProperty rdf:about="#isRealizedBy">
        <rdfs:comment rdf:datatype="&xsd;string"
            >The relation isRealizedBy states that a SystemFunction is implemented by some SystemRealization.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&system;isDirectlyRelatedTo"/>
        <rdfs:domain rdf:resource="#SystemFunction"/>
        <rdfs:range rdf:resource="#SystemRealization"/>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/technical_system.owl#realizes -->

    <owl:ObjectProperty rdf:about="#realizes">
        <rdfs:comment rdf:datatype="&xsd;string"
            >The relation realizes states that a SystemRealization implements a particular SystemFunction.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&system;isDirectlyRelatedTo"/>
        <rdfs:range rdf:resource="#SystemFunction"/>
        <rdfs:domain rdf:resource="#SystemRealization"/>
        <owl:inverseOf rdf:resource="#isRealizedBy"/>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/technical_system.owl#representsBehaviorOf -->

    <owl:ObjectProperty rdf:about="#representsBehaviorOf">
        <rdfs:comment rdf:datatype="&xsd;string"
            >The relation refers from a SystemBehavior to the overall TechnicalSystem.</rdfs:comment>
        <rdfs:range rdf:resource="&system;System"/>
        <rdfs:subPropertyOf rdf:resource="&system;representsAspectOf"/>
        <rdfs:domain rdf:resource="#SystemBehavior"/>
        <owl:inverseOf rdf:resource="#hasBehavioralAspect"/>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/technical_system.owl#representsFunctionOf -->

    <owl:ObjectProperty rdf:about="#representsFunctionOf">
        <rdfs:comment rdf:datatype="&xsd;string"
            >The relation refers from a SystemFunction to the overall TechnicalSystem.</rdfs:comment>
        <rdfs:range rdf:resource="&system;System"/>
        <rdfs:subPropertyOf rdf:resource="&system;representsAspectOf"/>
        <rdfs:domain rdf:resource="#SystemFunction"/>
        <owl:inverseOf rdf:resource="#hasFunctionalAspect"/>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/technical_system.owl#representsPerformanceOf -->

    <owl:ObjectProperty rdf:about="#representsPerformanceOf">
        <rdfs:comment rdf:datatype="&xsd;string"
            >The relation refers from a SystemPerformance to the overall TechnicalSystem.</rdfs:comment>
        <rdfs:range rdf:resource="&system;System"/>
        <rdfs:subPropertyOf rdf:resource="&system;representsAspectOf"/>
        <rdfs:domain rdf:resource="#SystemPerformance"/>
        <owl:inverseOf rdf:resource="#hasPerformanceAspect"/>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/technical_system.owl#representsRealizationOf -->

    <owl:ObjectProperty rdf:about="#representsRealizationOf">
        <rdfs:comment rdf:datatype="&xsd;string"
            >The relation refers from a SystemRealization to the overall TechnicalSystem.</rdfs:comment>
        <rdfs:range rdf:resource="&system;System"/>
        <rdfs:subPropertyOf rdf:resource="&system;representsAspectOf"/>
        <rdfs:domain rdf:resource="#SystemRealization"/>
        <owl:inverseOf rdf:resource="#hasRealizationAspect"/>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/technical_system.owl#representsRequirementsOf -->

    <owl:ObjectProperty rdf:about="#representsRequirementsOf">
        <rdfs:comment rdf:datatype="&xsd;string"
            >The relation refers from the SystemRequirements to the overall TechnicalSystem.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&system;representsAspectOf"/>
        <rdfs:domain rdf:resource="#SystemRequirements"/>
        <owl:inverseOf rdf:resource="#hasRequirementsAspect"/>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#object-featureRelation -->

    <owl:ObjectProperty rdf:about="&fundamental_concepts;object-featureRelation"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/system.owl#Aspect -->

    <owl:Class rdf:about="&system;Aspect">
        <owl:disjointWith rdf:resource="#Phenomenon"/>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/system.owl#AspectSystem -->

    <owl:Class rdf:about="&system;AspectSystem"/>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/system.owl#PhysicalDimension -->

    <owl:Class rdf:about="&system;PhysicalDimension">
        <owl:disjointWith rdf:resource="#Phenomenon"/>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/system.owl#Property -->

    <owl:Class rdf:about="&system;Property">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#isInfluencedBy"/>
                <owl:allValuesFrom rdf:resource="#Phenomenon"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/system.owl#System -->

    <owl:Class rdf:about="&system;System"/>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/system.owl#UnitOfMeasure -->

    <owl:Class rdf:about="&system;UnitOfMeasure">
        <owl:disjointWith rdf:resource="#Phenomenon"/>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/technical_system.owl#Phenomenon -->

    <owl:Class rdf:about="#Phenomenon">
        <rdfs:subClassOf rdf:resource="&fundamental_concepts;NonExhaustiveValueSet"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >A Phenomenon denotes a typical mode of behavior exhibited by a TechnicalSystem, thus providing a qualitative description of a recurring SystemBehavior.</rdfs:comment>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/technical_system.owl#SystemBehavior -->

    <owl:Class rdf:about="#SystemBehavior">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#representsBehaviorOf"/>
                <owl:someValuesFrom rdf:resource="#TechnicalSystem"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&system;AspectSystem"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&system;isConsideredUnderAspectOf"/>
                <owl:hasValue rdf:resource="#behavior"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasPhenomenon"/>
                <owl:allValuesFrom rdf:resource="#Phenomenon"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasPerformanceMeasure"/>
                <owl:allValuesFrom rdf:resource="#SystemPerformance"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#isConstrainedBy"/>
                <owl:allValuesFrom rdf:resource="#SystemRealization"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="#SystemFunction"/>
        <owl:disjointWith rdf:resource="#SystemPerformance"/>
        <owl:disjointWith rdf:resource="#SystemRealization"/>
        <owl:disjointWith rdf:resource="#SystemRequirements"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >The SystemBehavior describes how a TechnicalSystem operates under certain conditions; this description can be of qualitative or quantitative nature.</rdfs:comment>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/technical_system.owl#SystemFunction -->

    <owl:Class rdf:about="#SystemFunction">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#representsFunctionOf"/>
                <owl:someValuesFrom rdf:resource="#TechnicalSystem"/>
