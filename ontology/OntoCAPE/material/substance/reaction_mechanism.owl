        <owl:imports rdf:resource="https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/material/substance/molecular_structure.owl"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/material/substance/reaction_mechanism.owl#hasProduct -->

    <owl:ObjectProperty rdf:about="#hasProduct">
        <rdfs:comment rdf:datatype="&xsd;string"
            >The relation hasProduct denotes the products of a ChemicalReaction. A product is a MolecularEntity or a ChemicalSpecies that is formed during a ChemicalReaction.</rdfs:comment>
        <rdfs:domain rdf:resource="#ChemicalReaction"/>
        <rdfs:subPropertyOf rdf:resource="&system;isDirectlyRelatedTo"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&substance;ChemicalSpecies"/>
                    <rdf:Description rdf:about="&substance;MolecularEntity"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/material/substance/reaction_mechanism.owl#hasReactant -->

    <owl:ObjectProperty rdf:about="#hasReactant">
        <rdfs:comment rdf:datatype="&xsd;string"
            >The relation hasReactant denotes the reactants of a ChemicalReaction. A reactant is a MolecularEntity or a ChemicalSpecies that consumed in the course of a ChemicalReaction.</rdfs:comment>
        <rdfs:domain rdf:resource="#ChemicalReaction"/>
        <rdfs:subPropertyOf rdf:resource="&system;isDirectlyRelatedTo"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&substance;ChemicalSpecies"/>
                    <rdf:Description rdf:about="&substance;MolecularEntity"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/material/substance/reaction_mechanism.owl#hasStoichiometricCoefficient -->

    <owl:ObjectProperty rdf:about="#hasStoichiometricCoefficient">
        <rdfs:domain rdf:resource="#ChemicalReaction"/>
        <rdfs:range rdf:resource="#StoichiometricCoefficient"/>
        <rdfs:subPropertyOf rdf:resource="&fundamental_concepts;isOriginOf"/>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/material/substance/reaction_mechanism.owl#indicatesMultiplicityOf -->

    <owl:ObjectProperty rdf:about="#indicatesMultiplicityOf">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >The relation indicatesMultiplicityOf indicates the multiplicity of the reactants and products participating in a ChemicalReaction.</rdfs:comment>
        <rdfs:domain rdf:resource="#StoichiometricCoefficient"/>
        <rdfs:subPropertyOf rdf:resource="&fundamental_concepts;hasTargetObject"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&substance;ChemicalSpecies"/>
                    <rdf:Description rdf:about="&substance;MolecularEntity"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/system.owl#hasDirectSubsystem -->

    <owl:ObjectProperty rdf:about="&system;hasDirectSubsystem"/>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/system.owl#isComposedOfSubsystem -->

    <owl:ObjectProperty rdf:about="&system;isComposedOfSubsystem"/>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/system.owl#isDirectlyRelatedTo -->

    <owl:ObjectProperty rdf:about="&system;isDirectlyRelatedTo"/>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/system.owl#isExclusivelySubsystemOf -->

    <owl:ObjectProperty rdf:about="&system;isExclusivelySubsystemOf"/>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#hasTargetObject -->

    <owl:ObjectProperty rdf:about="&fundamental_concepts;hasTargetObject"/>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#isOriginOf -->

    <owl:ObjectProperty rdf:about="&fundamental_concepts;isOriginOf"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/material/substance/reaction_mechanism.owl#stoichiometricValue -->

    <owl:DatatypeProperty rdf:about="#stoichiometricValue">
        <rdfs:comment rdf:datatype="&xsd;string"
            >The attribute stoichiometricValue specifies the numerical value of a StoichiometricCoefficient. It is positive for products and negative for reactants.</rdfs:comment>
        <rdfs:domain rdf:resource="#StoichiometricCoefficient"/>
        <rdfs:subPropertyOf rdf:resource="&fundamental_concepts;relationAttribute"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </owl:DatatypeProperty>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#relationAttribute -->

    <owl:DatatypeProperty rdf:about="&fundamental_concepts;relationAttribute"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/material/substance/reaction_mechanism.owl#ChemicalReaction -->

    <owl:Class rdf:about="#ChemicalReaction">
        <rdfs:subClassOf rdf:resource="&system;System"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasStoichiometricCoefficient"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasReactant"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&substance;ChemicalSpecies"/>
                            <rdf:Description rdf:about="&substance;MolecularEntity"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasReactant"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasReactant"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&substance;ChemicalSpecies"/>
                            <rdf:Description rdf:about="&substance;MolecularEntity"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasProduct"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasStoichiometricCoefficient"/>
                <owl:allValuesFrom rdf:resource="#StoichiometricCoefficient"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasProduct"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&substance;ChemicalSpecies"/>
                            <rdf:Description rdf:about="&substance;MolecularEntity"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasProduct"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&substance;ChemicalSpecies"/>
                            <rdf:Description rdf:about="&substance;MolecularEntity"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string"
            >A ChemicalReaction converts some ChemicalSpecies (or MolecularEntities) into some other ChemicalSpecies (or MolecularEntities).</rdfs:comment>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/material/substance/reaction_mechanism.owl#CompositeReaction -->

    <owl:Class rdf:about="#CompositeReaction">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#SingleReaction"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&system;isComposedOfSubsystem"/>
                        <owl:someValuesFrom rdf:resource="#ElementaryReaction"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="#SingleReaction"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&system;isComposedOfSubsystem"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&system;isComposedOfSubsystem"/>
                <owl:allValuesFrom rdf:resource="#ElementaryReaction"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="#ElementaryReaction"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >A CompositeReaction is a ChemicalReaction that can be decomposed into several ElementaryReactions. Examples are parallel reactions (simultaneously occurring ElementaryReactions that form different products from a single set of reactants) and stepwise reactions (a set of consecutive ElementaryReactions with at least one reaction intermediate).</rdfs:comment>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/material/substance/reaction_mechanism.owl#ElementaryReaction -->

    <owl:Class rdf:about="#ElementaryReaction">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#SingleReaction"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&system;isExclusivelySubsystemOf"/>
                        <owl:someValuesFrom rdf:resource="#CompositeReaction"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&system;isExclusivelySubsystemOf"/>
                <owl:allValuesFrom rdf:resource="#CompositeReaction"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&system;isExclusivelySubsystemOf"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string"
            >An ElementaryReaction is a ChemicalReaction for which no reaction intermediates have been detected or need to be postulated in order to describe the ChemicalReaction on a molecular scale. An ElementaryReaction is assumed to occur in a single step and to pass through a single transition state (McNaught &amp; Wilkinson, 1997).</rdfs:comment>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/material/substance/reaction_mechanism.owl#IrreversibleReaction -->

    <owl:Class rdf:about="#IrreversibleReaction">
        <rdfs:subClassOf rdf:resource="#SingleReaction"/>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/material/substance/reaction_mechanism.owl#ReactionNetwork -->

    <owl:Class rdf:about="#ReactionNetwork">
        <rdfs:subClassOf rdf:resource="#ChemicalReaction"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&system;hasDirectSubsystem"/>
                <owl:allValuesFrom rdf:resource="#SingleReaction"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/material/substance/reaction_mechanism.owl#ReversibleReaction -->

    <owl:Class rdf:about="#ReversibleReaction">
        <rdfs:subClassOf rdf:resource="#SingleReaction"/>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/material/substance/reaction_mechanism.owl#SingleReaction -->

    <owl:Class rdf:about="#SingleReaction">
        <rdfs:subClassOf rdf:resource="#ChemicalReaction"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasStoichiometricCoefficient"/>
                <owl:someValuesFrom rdf:resource="#StoichiometricCoefficient"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/material/substance/reaction_mechanism.owl#StoichiometricCoefficient -->

    <owl:Class rdf:about="#StoichiometricCoefficient">
        <rdfs:subClassOf rdf:resource="#StoichiometricCoefficient"/>
        <rdfs:subClassOf rdf:resource="&fundamental_concepts;UniqueOriginN-aryRelation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#indicatesMultiplicityOf"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#indicatesMultiplicityOf"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&substance;ChemicalSpecies"/>
                            <rdf:Description rdf:about="&substance;MolecularEntity"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#indicatesMultiplicityOf"/>
                <owl:allValuesFrom rdf:resource="&substance;ChemicalComponent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#stoichiometricValue"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string"
            >A StoichiometricCoefficient indicates the multiplicity of a ChemicalSpecies or MolecularEntity that participates in a ChemicalReaction.</rdfs:comment>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/material/substance/substance.owl#ChemicalComponent -->

    <owl:Class rdf:about="&substance;ChemicalComponent"/>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/material/substance/substance.owl#ChemicalSpecies -->

    <owl:Class rdf:about="&substance;ChemicalSpecies"/>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/material/substance/substance.owl#MolecularEntity -->

    <owl:Class rdf:about="&substance;MolecularEntity"/>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/upper_level/system.owl#System -->

