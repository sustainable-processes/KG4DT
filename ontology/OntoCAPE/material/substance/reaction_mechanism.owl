    <!ENTITY der_dim "https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/dev-alkyne-hydrogenation/ontology/OntoCAPE/supporting_concepts/physical_dimension/derived_dimensions.owl#" >
]>


