    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/main/ontology/OntoCAPE/supporting_concepts/mathematical_relation/mathematical_relation.owl#divide -->

    <rdf:Description rdf:about="&maths;divide"/>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/main/ontology/OntoCAPE/supporting_concepts/mathematical_relation/mathematical_relation.owl#power -->

    <rdf:Description rdf:about="&maths;power"/>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/main/ontology/OntoCAPE/supporting_concepts/mathematical_relation/mathematical_relation.owl#times -->

    <rdf:Description rdf:about="&maths;times"/>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/main/ontology/OntoCAPE/supporting_concepts/physical_dimension/derived_dimensions.owl#ACCELERATION -->

    <maths:Node rdf:about="#ACCELERATION">
        <rdfs:comment rdf:datatype="&xsd;string">velocity/time</rdfs:comment>
        <maths:hasNodeValue rdf:resource="&maths;divide"/>
        <maths:hasLeftChild rdf:resource="#VELOCITY"/>
        <maths:hasRightChild rdf:resource="&phys_dim;TIME"/>
    </maths:Node>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/main/ontology/OntoCAPE/supporting_concepts/physical_dimension/derived_dimensions.owl#AREA -->

    <maths:Node rdf:about="#AREA">
        <maths:rightChildNodeValue rdf:datatype="&xsd;decimal">2</maths:rightChildNodeValue>
        <rdfs:comment rdf:datatype="&xsd;string">length^2</rdfs:comment>
        <maths:hasNodeValue rdf:resource="&maths;power"/>
        <maths:hasLeftChild rdf:resource="&phys_dim;LENGTH"/>
    </maths:Node>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/main/ontology/OntoCAPE/supporting_concepts/physical_dimension/derived_dimensions.owl#ELECTRIC_CHARGE -->

    <maths:Node rdf:about="#ELECTRIC_CHARGE">
        <maths:hasNodeValue rdf:resource="&maths;times"/>
        <maths:hasLeftChild rdf:resource="&phys_dim;ELECTRIC_CURRENT"/>
        <maths:hasRightChild rdf:resource="&phys_dim;TIME"/>
    </maths:Node>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/main/ontology/OntoCAPE/supporting_concepts/physical_dimension/derived_dimensions.owl#MOLAR_VOLUME -->

    <maths:Node rdf:about="#MOLAR_VOLUME">
        <rdfs:comment rdf:datatype="&xsd;string"
            >volume/amount_of_substance</rdfs:comment>
        <maths:hasNodeValue rdf:resource="&maths;divide"/>
        <maths:hasLeftChild rdf:resource="#VOLUME"/>
        <maths:hasRightChild rdf:resource="&phys_dim;AMOUNT_OF_SUBSTANCE"/>
    </maths:Node>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/main/ontology/OntoCAPE/supporting_concepts/physical_dimension/derived_dimensions.owl#MOLECULAR_MASS -->

    <maths:Node rdf:about="#MOLECULAR_MASS">
        <rdfs:comment rdf:datatype="&xsd;string"
            >mass/amount_of_substance</rdfs:comment>
        <maths:hasNodeValue rdf:resource="&maths;divide"/>
        <maths:hasRightChild rdf:resource="&phys_dim;AMOUNT_OF_SUBSTANCE"/>
        <maths:hasLeftChild rdf:resource="&phys_dim;MASS"/>
    </maths:Node>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/main/ontology/OntoCAPE/supporting_concepts/physical_dimension/derived_dimensions.owl#VELOCITY -->

    <maths:Node rdf:about="#VELOCITY">
        <rdfs:comment rdf:datatype="&xsd;string">length/time</rdfs:comment>
        <maths:hasNodeValue rdf:resource="&maths;divide"/>
        <maths:hasLeftChild rdf:resource="&phys_dim;LENGTH"/>
        <maths:hasRightChild rdf:resource="&phys_dim;TIME"/>
    </maths:Node>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/main/ontology/OntoCAPE/supporting_concepts/physical_dimension/derived_dimensions.owl#VOLUME -->

    <maths:Node rdf:about="#VOLUME">
        <maths:rightChildNodeValue rdf:datatype="&xsd;decimal">3</maths:rightChildNodeValue>
        <rdfs:comment rdf:datatype="&xsd;string">length^3</rdfs:comment>
        <maths:hasNodeValue rdf:resource="&maths;power"/>
        <maths:isLeftChildOf rdf:resource="#MOLAR_VOLUME"/>
        <maths:hasLeftChild rdf:resource="&phys_dim;LENGTH"/>
    </maths:Node>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/main/ontology/OntoCAPE/supporting_concepts/physical_dimension/derived_dimensions.owl#acceleration -->

    <SpaceAndTime rdf:about="#acceleration">
        <rdfs:comment rdf:datatype="&xsd;string"
            >the first time derivative from velocity [Chertov, 1997]</rdfs:comment>
        <phys_dim:isDefinedBy rdf:resource="#ACCELERATION"/>
        <phys_dim:isDefinedBy rdf:resource="#AREA"/>
    </SpaceAndTime>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/main/ontology/OntoCAPE/supporting_concepts/physical_dimension/derived_dimensions.owl#angular_acceleration -->

    <SpaceAndTime rdf:about="#angular_acceleration">
        <rdfs:comment rdf:datatype="&xsd;string"
            >the first time derivative from the angular velocity [Chertov, 1997]</rdfs:comment>
    </SpaceAndTime>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/main/ontology/OntoCAPE/supporting_concepts/physical_dimension/derived_dimensions.owl#angular_velocity -->

    <SpaceAndTime rdf:about="#angular_velocity">
        <rdfs:comment rdf:datatype="&xsd;string"
            >the first time derivative from the deflection angle [Chertov, 1997]</rdfs:comment>
    </SpaceAndTime>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/main/ontology/OntoCAPE/supporting_concepts/physical_dimension/derived_dimensions.owl#area -->

    <SpaceAndTime rdf:about="#area">
        <rdfs:comment rdf:datatype="&xsd;string"
            >characterizes the plane and curved surfaces of a physical body [Chertov, 1997]</rdfs:comment>
        <phys_dim:isDefinedBy rdf:resource="#AREA"/>
    </SpaceAndTime>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/main/ontology/OntoCAPE/supporting_concepts/physical_dimension/derived_dimensions.owl#damping_coefficient -->

    <PeriodicPhenomena rdf:about="#damping_coefficient">
        <rdfs:comment rdf:datatype="&xsd;string"
            >the inverse to the time interval during which the amplitude decrease e times [Chertov, 1997]</rdfs:comment>
    </PeriodicPhenomena>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/main/ontology/OntoCAPE/supporting_concepts/physical_dimension/derived_dimensions.owl#density -->

    <Mechanics rdf:about="#density">
        <rdfs:comment rdf:datatype="&xsd;string"
            >the ratio of the mass to the volume of a body&#180;s element [Chertov, 1997]</rdfs:comment>
    </Mechanics>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/main/ontology/OntoCAPE/supporting_concepts/physical_dimension/derived_dimensions.owl#dynamic_viscosity -->

    <Mechanics rdf:about="#dynamic_viscosity">
        <rdfs:comment rdf:datatype="&xsd;string"
            >the coefficient of proportinality in the internal friction force formula [Chertov, 1997]</rdfs:comment>
    </Mechanics>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/main/ontology/OntoCAPE/supporting_concepts/physical_dimension/derived_dimensions.owl#electric_charge -->

    <ElectricityAndMagnetism rdf:about="#electric_charge">
        <rdfs:comment rdf:datatype="&xsd;string"
            >The measure equal to the product of electic current into time during which the current flows [Chertov]</rdfs:comment>
        <phys_dim:isDefinedBy rdf:resource="#ELECTRIC_CHARGE"/>
    </ElectricityAndMagnetism>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/main/ontology/OntoCAPE/supporting_concepts/physical_dimension/derived_dimensions.owl#entropy -->

    <Thermodynamics rdf:about="#entropy">
        <rdfs:comment rdf:datatype="&xsd;string"
            >the ratio of an infinitely small quantity of heat transmitted to a system to the temperature [Chertov, 1997]</rdfs:comment>
    </Thermodynamics>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/main/ontology/OntoCAPE/supporting_concepts/physical_dimension/derived_dimensions.owl#force -->

    <Mechanics rdf:about="#force">
        <rdfs:comment rdf:datatype="&xsd;string"
            >the degree of mechanical influence exerted on a body by other bodies [Chertov, 1997]</rdfs:comment>
    </Mechanics>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/main/ontology/OntoCAPE/supporting_concepts/physical_dimension/derived_dimensions.owl#frequency -->

    <PeriodicPhenomena rdf:about="#frequency">
        <rdfs:comment rdf:datatype="&xsd;string"
            >the inverse to the period [Chertov, 1997]</rdfs:comment>
    </PeriodicPhenomena>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/main/ontology/OntoCAPE/supporting_concepts/physical_dimension/derived_dimensions.owl#heat_capacity -->

    <Thermodynamics rdf:about="#heat_capacity">
        <rdfs:comment rdf:datatype="&xsd;string"
            >the ratio of heat required to warm a physical body to the difference in temperature [Chertov, 1997]</rdfs:comment>
    </Thermodynamics>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/main/ontology/OntoCAPE/supporting_concepts/physical_dimension/derived_dimensions.owl#heat_flow_rate -->

