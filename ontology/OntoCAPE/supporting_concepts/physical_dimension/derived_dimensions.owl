    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/main/ontology/OntoCAPE/supporting_concepts/mathematical_relation/mathematical_relation.owl#Node -->

    <owl:Class rdf:about="&maths;Node"/>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/main/ontology/OntoCAPE/supporting_concepts/physical_dimension/derived_dimensions.owl#ElectricityAndMagnetism -->

    <owl:Class rdf:about="#ElectricityAndMagnetism">
        <rdfs:subClassOf rdf:resource="&phys_dim;DerivedDimension"/>
        <owl:disjointWith rdf:resource="#Mechanics"/>
        <owl:disjointWith rdf:resource="#PeriodicPhenomena"/>
        <owl:disjointWith rdf:resource="#SpaceAndTime"/>
        <owl:disjointWith rdf:resource="#Thermodynamics"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >The class ElectricityAndMagnetism subsumes DerivedDimensions that are connected with the phenomena of electricity or magnetism.</rdfs:comment>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/main/ontology/OntoCAPE/supporting_concepts/physical_dimension/derived_dimensions.owl#Mechanics -->

    <owl:Class rdf:about="#Mechanics">
        <rdfs:subClassOf rdf:resource="&phys_dim;DerivedDimension"/>
        <owl:disjointWith rdf:resource="#PeriodicPhenomena"/>
        <owl:disjointWith rdf:resource="#SpaceAndTime"/>
        <owl:disjointWith rdf:resource="#Thermodynamics"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >The class Mechanics subsumes DerivedDimensions that are of relevance for the field of mechanics.</rdfs:comment>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/main/ontology/OntoCAPE/supporting_concepts/physical_dimension/derived_dimensions.owl#PeriodicPhenomena -->

    <owl:Class rdf:about="#PeriodicPhenomena">
        <rdfs:subClassOf rdf:resource="&phys_dim;DerivedDimension"/>
        <owl:disjointWith rdf:resource="#SpaceAndTime"/>
        <owl:disjointWith rdf:resource="#Thermodynamics"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >The class PeriodicPhenomena subsumes DerivedDimensions with a periodic character, such as frequency or period.</rdfs:comment>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/main/ontology/OntoCAPE/supporting_concepts/physical_dimension/derived_dimensions.owl#SpaceAndTime -->

    <owl:Class rdf:about="#SpaceAndTime">
        <rdfs:subClassOf rdf:resource="&phys_dim;DerivedDimension"/>
        <owl:disjointWith rdf:resource="#Thermodynamics"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >The class SpaceAndTime subsumes DerivedDimensions that can be derived from the base dimensions length and time.</rdfs:comment>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/main/ontology/OntoCAPE/supporting_concepts/physical_dimension/derived_dimensions.owl#Thermodynamics -->

    <owl:Class rdf:about="#Thermodynamics">
        <rdfs:subClassOf rdf:resource="&phys_dim;DerivedDimension"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >The class Thermodynamics subsumes DerivedDimensions that are of relevance in the area of heat transfer and thermodynamics.</rdfs:comment>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/main/ontology/OntoCAPE/supporting_concepts/physical_dimension/physical_dimension.owl#DerivedDimension -->

    <owl:Class rdf:about="&phys_dim;DerivedDimension"/>
    


