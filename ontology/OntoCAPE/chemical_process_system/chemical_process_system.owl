  <owl:Class rdf:ID="ProcessUnit">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <rdf:Description rdf:about="&behavior;#MaterialAmount">
            <rdfs:subClassOf>
              <owl:Restriction>
                <owl:onProperty rdf:resource="&system;#isConsideredUnderAspectOf"/>
                <owl:hasValue rdf:resource="&technical_system;#behavior"/>
              </owl:Restriction>
            </rdfs:subClassOf>
          </rdf:Description>
        </owl:someValuesFrom>
        <owl:onProperty rdf:resource="&technical_system;#hasBehavioralAspect"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <rdf:Description rdf:about="&plant;#PlantItem">
            <rdfs:subClassOf>
              <owl:Restriction>
                <owl:hasValue rdf:resource="&technical_system;#realization"/>
                <owl:onProperty rdf:resource="&system;#isConsideredUnderAspectOf"/>
              </owl:Restriction>
            </rdfs:subClassOf>
            <rdfs:subClassOf>
              <owl:Restriction>
                <owl:onProperty rdf:resource="&technical_system;#realizes"/>
                <owl:allValuesFrom>
                  <rdf:Description rdf:about="&process;#ProcessStep">
                    <rdfs:subClassOf>
                      <owl:Restriction>
                        <owl:hasValue rdf:resource="&technical_system;#function"/>
                        <owl:onProperty rdf:resource="&system;#isConsideredUnderAspectOf"/>
                      </owl:Restriction>
                    </rdfs:subClassOf>
                  </rdf:Description>
                </owl:allValuesFrom>
              </owl:Restriction>
            </rdfs:subClassOf>
          </rdf:Description>
        </owl:someValuesFrom>
        <owl:onProperty rdf:resource="&technical_system;#hasRealizationAspect"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&technical_system;#hasRealizationAspect"/>
        <owl:allValuesFrom rdf:resource="&plant;#PlantItem"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="&system;#System"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&technical_system;#hasFunctionalAspect"/>
        <owl:allValuesFrom rdf:resource="&process;#ProcessStep"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&technical_system;#hasPerformanceAspect"/>
        <owl:someValuesFrom>
          <rdf:Description rdf:about="&econ_perf;#EconomicPerformance">
            <rdfs:subClassOf>
              <owl:Restriction>
                <owl:allValuesFrom rdf:resource="&plant;#PlantItem"/>
                <owl:onProperty rdf:resource="&technical_system;#evaluates"/>
              </owl:Restriction>
            </rdfs:subClassOf>
            <rdfs:subClassOf>
              <owl:Restriction>
                <owl:onProperty rdf:resource="&system;#isConsideredUnderAspectOf"/>
                <owl:hasValue rdf:resource="&technical_system;#performance"/>
              </owl:Restriction>
            </rdfs:subClassOf>
          </rdf:Description>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="&xsd;#string"
    >ProcessUnit comprises ChemicaProcessSystem parts that are considered under specific aspects but do not represent the entire ChemicalProcessSystem.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&technical_system;#hasFunctionalAspect"/>
        <owl:someValuesFrom rdf:resource="&process;#ProcessStep"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="&econ_perf;#EconomicPerformance"/>
        <owl:onProperty rdf:resource="&technical_system;#hasPerformanceAspect"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="&behavior;#MaterialAmount"/>
        <owl:onProperty rdf:resource="&technical_system;#hasBehavioralAspect"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="ChemicalProcessSystem">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&technical_system;#hasFunctionalAspect"/>
        <owl:someValuesFrom rdf:resource="&process;#Process"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&technical_system;#hasRealizationAspect"/>
        <owl:someValuesFrom rdf:resource="&plant;#Plant"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="&econ_perf;#EconomicPerformance"/>
        <owl:onProperty rdf:resource="&technical_system;#hasPerformanceAspect"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="&system;#System"/>
    <rdfs:comment rdf:datatype="&xsd;#string"
    >The ChemicalProcessSystem is a TechnicalSystem which is designed, built, and run in order to produce a chemical compound.</rdfs:comment>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="refersToGeneralizedAmount">
    <rdfs:domain>
      <rdf:Description rdf:about="&process;#ProcessState">
        <rdfs:subClassOf>
          <owl:Restriction>
            <owl:hasValue rdf:resource="&technical_system;#function"/>
            <owl:onProperty rdf:resource="&system;#isConsideredUnderAspectOf"/>
          </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
          <owl:Restriction>
            <owl:someValuesFrom rdf:resource="&behavior;#GeneralizedAmount"/>
            <owl:onProperty rdf:resource="&chemical_process_system;#refersToGeneralizedAmount"/>
          </owl:Restriction>
        </rdfs:subClassOf>
      </rdf:Description>
    </rdfs:domain>
    <rdfs:range rdf:resource="&behavior;#GeneralizedAmount"/>
  </owl:ObjectProperty>
  <rdf:Description rdf:about="&process;#Leaching">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&network_system;#hasInput"/>
        <owl:someValuesFrom>
          <owl:Class>
            <owl:intersectionOf rdf:parseType="Collection">
              <rdf:Description rdf:about="&process;#ProcessState"/>
              <owl:Restriction>
                <owl:allValuesFrom>
                  <owl:Class>
                    <owl:intersectionOf rdf:parseType="Collection">
                      <rdf:Description rdf:about="&behavior;#MaterialStream"/>
                      <owl:Restriction>
                        <owl:someValuesFrom>
                          <owl:Class>
                            <owl:intersectionOf rdf:parseType="Collection">
                              <rdf:Description rdf:about="&material;#Material"/>
                              <owl:Restriction>
                                <owl:onProperty rdf:resource="&material;#thermodynamicBehavior"/>
                                <owl:someValuesFrom>
                                  <owl:Class>
                                    <owl:intersectionOf rdf:parseType="Collection">
                                      <rdf:Description rdf:about="&phase_system;#SinglePhase"/>
                                      <owl:Restriction>
                                        <owl:hasValue rdf:resource="&phase_system;#liquid"/>
                                        <owl:onProperty rdf:resource="&phase_system;#hasStateOfAggregation"/>
                                      </owl:Restriction>
                                    </owl:intersectionOf>
                                  </owl:Class>
                                </owl:someValuesFrom>
                              </owl:Restriction>
                            </owl:intersectionOf>
                          </owl:Class>
                        </owl:someValuesFrom>
                        <owl:onProperty rdf:resource="&behavior;#refersToMaterial"/>
                      </owl:Restriction>
                    </owl:intersectionOf>
                  </owl:Class>
                </owl:allValuesFrom>
                <owl:onProperty rdf:resource="&chemical_process_system;#refersToGeneralizedAmount"/>
              </owl:Restriction>
              <owl:Restriction>
                <owl:cardinality rdf:datatype="&xsd;#int"
                >1</owl:cardinality>
                <owl:onProperty rdf:resource="&chemical_process_system;#refersToGeneralizedAmount"/>
              </owl:Restriction>
            </owl:intersectionOf>
          </owl:Class>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class>
            <owl:intersectionOf rdf:parseType="Collection">
              <rdf:Description rdf:about="&process;#ProcessState"/>
              <owl:Restriction>
                <owl:onProperty rdf:resource="&chemical_process_system;#refersToGeneralizedAmount"/>
                <owl:allValuesFrom>
                  <owl:Class>
                    <owl:intersectionOf rdf:parseType="Collection">
                      <rdf:Description rdf:about="&behavior;#MaterialStream"/>
                      <owl:Restriction>
                        <owl:someValuesFrom>
                          <owl:Class>
                            <owl:intersectionOf rdf:parseType="Collection">
