    <rdfs:subPropertyOf rdf:resource="&multiset;#indicatesMultiplicityOf"/>
    <rdfs:range rdf:resource="&plant;#PlantItem"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasCapacity">
