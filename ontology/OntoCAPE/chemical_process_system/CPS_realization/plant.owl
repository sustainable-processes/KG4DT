    <rdfs:range rdf:resource="&xsd;#int"/>
    <rdfs:subPropertyOf rdf:resource="&multiset;#multiplicity"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="has_number_of_tube_passes_per_shell">
    <rdfs:range rdf:resource="&xsd;#int"/>
  </owl:DatatypeProperty>
  <owl:AllDifferent/>
</rdf:RDF>
