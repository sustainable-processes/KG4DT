    <rdfs:subPropertyOf rdf:resource="&multiset;#hasMultiplicity"/>
    <rdfs:domain rdf:resource="&plant;#PlantItem"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasNumberOfPlantItems">
