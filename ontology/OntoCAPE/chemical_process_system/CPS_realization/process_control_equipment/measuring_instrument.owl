  <owl:Class rdf:ID="Thermocouple">
    <rdfs:comment rdf:datatype="&xsd;#string"
    >Thermocouple is a specific type of temperature measuring sensor.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="T-Sensor"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="SegerCone">
    <rdfs:comment rdf:datatype="&xsd;#string"
    >SegerCone is a specific type of temperature measuring sensor.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/main/ontology/OntoCAPE/chemical_process_system/CPS_realization/process_control_equipment/measuring_instrument.owl#T-Sensor"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Pt100">
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://raw.githubusercontent.com/sustainable-processes/KG4DT/refs/heads/main/ontology/OntoCAPE/chemical_process_system/CPS_realization/process_control_equipment/measuring_instrument.owl#T-Sensor"/>
    </rdfs:subClassOf>
