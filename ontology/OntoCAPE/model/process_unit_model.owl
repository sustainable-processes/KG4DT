                You should have received a copy of the GNU General Public License along with this program; if not, see &lt;http://www.gnu.org/licenses/&gt;.
</terms:license>
  </owl:Ontology>
 
  <owl:Class rdf:ID="TrayModel">
    <rdfs:subClassOf rdf:resource="&mathematical_model;#Submodel"/>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="FlashUnitModel"/>
    </rdfs:subClassOf>
  </owl:Class>
 
  <owl:Class rdf:ID="TrayByTrayDistillationColumnModel">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:onProperty rdf:resource="&system;#models"/>
            <owl:allValuesFrom rdf:resource="&distillation_system;#DistillationSystemBehavior"/>
          </owl:Restriction>
          <owl:Class rdf:ID="ProcessUnitModel"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&process_model;#first-principles"/>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="&process_model;#hasModelingPrinciple"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="&laws;#MassConservationLaw"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="&system;#hasDirectSubsystem"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="&laws;#EnergyConservationLaw"/>
        <owl:onProperty rdf:resource="&system;#hasDirectSubsystem"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="&process_unit_model;#ProcessUnitModel">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:allValuesFrom rdf:resource="&behavior;#MaterialAmount"/>
            <owl:onProperty rdf:resource="&system;#models"/>
          </owl:Restriction>
          <rdf:Description rdf:about="&process_model;#ProcessModel"/>
          <owl:Restriction>
            <owl:onProperty rdf:resource="&system;#models"/>
            <owl:someValuesFrom rdf:resource="&behavior;#MaterialAmount"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="CSTRModel">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="&laws;#MassConservationLaw"/>
        <owl:onProperty rdf:resource="&system;#hasDirectSubsystem"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="&process_model;#hasModelingPrinciple"/>
        </owl:onProperty>
        <owl:hasValue rdf:resource="&process_model;#first-principles"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="&laws;#ReactionKineticsLaw"/>
        <owl:onProperty rdf:resource="&system;#hasDirectSubsystem"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&system;#hasDirectSubsystem"/>
        <owl:someValuesFrom rdf:resource="&laws;#EnergyConservationLaw"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:onProperty rdf:resource="&system;#models"/>
            <owl:allValuesFrom>
              <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                  <rdf:Description rdf:about="&chemical_reactor;#ChemicalReactorBehavior"/>
                  <owl:Restriction>
                    <owl:onProperty rdf:resource="&technical_system;#hasPhenomenon"/>
                    <owl:hasValue rdf:resource="&behavior;#ideal_mixing"/>
                  </owl:Restriction>
                </owl:intersectionOf>
              </owl:Class>
            </owl:allValuesFrom>
          </owl:Restriction>
          <owl:Class rdf:ID="ChemicalReactorModel"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="&process_unit_model;#ChemicalReactorModel">
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="&chemical_reactor;#ChemicalReactorBehavior"/>
        <owl:onProperty rdf:resource="&system;#models"/>
      </owl:Restriction>
    </owl:equivalentClass>
    <rdfs:subClassOf rdf:resource="&process_unit_model;#ProcessUnitModel"/>
  </owl:Class>
  
  <owl:Class rdf:ID="HeatTransferUnitModel">
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="&heat_transfer_unit;#HeatTransferUnitBehavior"/>
        <owl:onProperty rdf:resource="&system;#models"/>
      </owl:Restriction>
    </owl:equivalentClass>
    <rdfs:subClassOf rdf:resource="&process_unit_model;#ProcessUnitModel"/>
  </owl:Class>
  
  <owl:Class rdf:about="&process_unit_model;#FlashUnitModel">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&system;#hasDirectSubsystem"/>
        <owl:someValuesFrom rdf:resource="&laws;#MassConservationLaw"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&process_model;#first-principles"/>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="&process_model;#hasModelingPrinciple"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="&laws;#PhaseEquilibriumLaw"/>
        <owl:onProperty rdf:resource="&system;#hasDirectSubsystem"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="&laws;#EnergyConservationLaw"/>
        <owl:onProperty rdf:resource="&system;#hasDirectSubsystem"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:onProperty rdf:resource="&system;#models"/>
            <owl:allValuesFrom rdf:resource="&flash_unit;#FlashUnitBehavior"/>
          </owl:Restriction>
          <owl:Class rdf:about="&process_unit_model;#ProcessUnitModel"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <owl:disjointWith>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="&laws;#NonEquilibriumLaw"/>
        <owl:onProperty rdf:resource="&system;#hasDirectSubsystem"/>
      </owl:Restriction>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="PFRModel">
    <rdfs:comment rdf:datatype="&xsd;#string"
    >Model of an ideal Plug Flow Reactor</rdfs:comment>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:allValuesFrom>
              <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                  <rdf:Description rdf:about="&chemical_reactor;#ChemicalReactorBehavior"/>
                  <owl:Restriction>
                    <owl:onProperty rdf:resource="&technical_system;#hasPhenomenon"/>
                    <owl:hasValue rdf:resource="&behavior;#turbulent_flow"/>
                  </owl:Restriction>
                </owl:intersectionOf>
              </owl:Class>
            </owl:allValuesFrom>
            <owl:onProperty rdf:resource="&system;#models"/>
          </owl:Restriction>
          <owl:Class rdf:about="&process_unit_model;#ChemicalReactorModel"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="&laws;#EnergyConservationLaw"/>
        <owl:onProperty rdf:resource="&system;#hasDirectSubsystem"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&system;#hasDirectSubsystem"/>
        <owl:someValuesFrom rdf:resource="&laws;#ReactionKineticsLaw"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="&laws;#MassConservationLaw"/>
        <owl:onProperty rdf:resource="&system;#hasDirectSubsystem"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="&process_model;#hasModelingPrinciple"/>
        </owl:onProperty>
        <owl:hasValue rdf:resource="&process_model;#first-principles"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:FunctionalProperty rdf:about="&process_model;#hasModelingPrinciple">
    <rdf:type rdf:resource="&owl;#ObjectProperty"/>
  </owl:FunctionalProperty>
</rdf:RDF>
