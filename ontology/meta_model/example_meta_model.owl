  <!ENTITY example_linked_list  "&root;meta_model/data_structures/example_linked_list.owl">
  <!ENTITY example_loop         "&root;meta_model/data_structures/example_loop.owl">
  <!ENTITY example_meta         "&root;meta_model/example_meta_model.owl">
