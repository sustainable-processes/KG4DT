    <owl:imports rdf:resource="&example_linked_list;"/>
    <owl:imports rdf:resource="&example_multiset;"/>
    <owl:imports rdf:resource="&example_array;"/>
    <owl:imports rdf:resource="&example_binary_tree;"/>
    <owl:imports rdf:resource="&example_loop;"/>
