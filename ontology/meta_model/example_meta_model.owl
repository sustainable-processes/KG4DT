    xmlns:example_mereology="&example_mereology;#"
    xmlns:example_multiset="&example_multiset;#"
    xmlns:example_topology="&example_topology;#"
    
    xml:base="&example_meta;">
  
  <owl:Ontology rdf:about="&example_meta;">
    <owl:imports rdf:resource="&example_mereology;"/>
