&lt;phys_dim:BaseDimension       rdf:about="&phys_dim;#mass"/&gt;
&lt;phys_dim:BaseDimension       rdf:about="&phys_dim;#thermodynamic_temperature"/&gt;
&lt;phys_dim:BaseDimension       rdf:about="&phys_dim;#time"/&gt;
    </rdfs:comment>
