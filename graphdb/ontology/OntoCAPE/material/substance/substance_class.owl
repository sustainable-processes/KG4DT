<?xml version="1.0"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY root "file:/ontology/">
  <!ENTITY owl  "http://www.w3.org/2002/07/owl">
  <!ENTITY xsd  "http://www.w3.org/2001/XMLSchema">
  <!ENTITY rdf  "http://www.w3.org/1999/02/22-rdf-syntax-ns">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema">
  <!ENTITY system   "&root;OntoCAPE/upper_level/system.owl">
  <!ENTITY chemical_species "&root;OntoCAPE/material/substance/chemical_species.owl">
  <!ENTITY molecular_structure  "&root;OntoCAPE/material/substance/molecular_structure.owl">
  <!ENTITY substance    "&root;OntoCAPE/material/substance/substance.owl">
  <!ENTITY substance_class  "&root;OntoCAPE/material/substance/substance_class.owl">
]>

<rdf:RDF
    xmlns:molecular_structure="&molecular_structure;#"
    xmlns:substance="&substance;#"
    xmlns:rdf="&rdf;#"
    xmlns:xsd="&xsd;#"
    xmlns:rdfs="&rdfs;#"
    xmlns:owl="&owl;#"
    xmlns:terms="http://purl.org/dc/terms/"
    xmlns="&substance_class;#"
    xmlns:chemical_species="&chemical_species;#"
  xml:base="&substance_class;">
  <owl:Ontology rdf:about="&substance_class;">
    <owl:imports rdf:resource="&molecular_structure;"/>
    <owl:imports rdf:resource="&chemical_species;"/>
    <rdfs:comment rdf:datatype="&xsd;#string"
    >The module 'substance_class' is concerned with the classification of pure substances.
    The follwoing classes, relations, and individuals from other ontology modules are used within 'substance_class':

&lt;owl:Class rdf:about="&molecular_structure;#Ion"/>
&lt;owl:Class rdf:about="&molecular_structure;#MolecularGroup"/>
&lt;owl:Class rdf:about="&substance;#ChemicalSpecies"/>
&lt;owl:Class rdf:about="&substance;#Mixture"/>
&lt;owl:Class rdf:about="&substance;#MolecularEntity"/>

&lt;owl:ObjectProperty rdf:about="&system;#contains"/>
&lt;owl:ObjectProperty rdf:about="&system;#containsDirectly"/>
&lt;owl:ObjectProperty rdf:about="&substance;#hasMolecularStructure"/>
&lt;owl:DatatypeProperty rdf:about="&substance;#structuralFormula"/>

&lt;substance:ChemicalSpecies rdf:about="&chemical_species;#_1-Butanol"/>
&lt;substance:ChemicalSpecies rdf:about="&chemical_species;#Acetic__acid"/>
&lt;substance:ChemicalSpecies rdf:about="&chemical_species;#Ethanol"/>
&lt;substance:ChemicalSpecies rdf:about="&chemical_species;#Ethyl__acetate"/>
&lt;substance:ChemicalSpecies rdf:about="&chemical_species;#Hydrogen__bromide"/>
&lt;substance:ChemicalSpecies rdf:about="&chemical_species;#Hydrogen__chloride"/>
&lt;substance:ChemicalSpecies rdf:about="&chemical_species;#Hydrogen__fluoride"/>
&lt;substance:ChemicalSpecies rdf:about="&chemical_species;#Hydrogen__iodide"/>
&lt;substance:ChemicalSpecies rdf:about="&chemical_species;#Methyl__alcohol"/>
&lt;substance:ChemicalSpecies rdf:about="&chemical_species;#Nitric__acid"/>
&lt;substance:ChemicalSpecies rdf:about="&chemical_species;#Phosphoric__acid"/>
&lt;substance:ChemicalSpecies rdf:about="&chemical_species;#Sulfuric__acid"/>
</rdfs:comment>
