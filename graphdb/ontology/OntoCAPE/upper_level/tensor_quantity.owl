        <owl:allValuesFrom rdf:resource="&system;#QuantitativeValue"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="&system;#QuantitativeValue"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="&tensor_quantity;#hasElement"/>
        </owl:onProperty>
        <owl:minCardinality rdf:datatype="&xsd;#int"
        >2</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="&xsd;#string"
    >A TensorValue is non-scalar QuantitativeValue of a TensorQuantity.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:minCardinality rdf:datatype="&xsd;#int"
        >2</owl:minCardinality>
        <owl:onProperty>
          <owl:InverseFunctionalProperty rdf:about="&tensor_quantity;#isOrderedBy"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:about="&tensor_quantity;#Index"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:InverseFunctionalProperty rdf:about="&tensor_quantity;#isOrderedBy"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="&tensor_quantity;#TensorQuantity">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="&system;#PhysicalQuantity"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="&tensor_quantity;#hasElement"/>
        </owl:onProperty>
      </owl:Restriction>
