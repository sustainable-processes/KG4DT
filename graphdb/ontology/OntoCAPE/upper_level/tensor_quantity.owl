  </owl:Ontology>
  
    <owl:Class rdf:ID="ColumnIndex">
    <owl:disjointWith>
      <owl:Class rdf:ID="RowIndex"/>
    </owl:disjointWith>
    <rdfs:comment rdf:datatype="&xsd;#string"
    >A ColumnIndex denotes the position of a column vector within a matrix.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:ID="determinesPositionOf"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <owl:Class rdf:ID="ColumnVectorQuantity"/>
              <owl:Class rdf:ID="ColumnVectorValue"/>
            </owl:unionOf>
          </owl:Class>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="isIndexOf"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <owl:Class rdf:ID="MatrixQuantity"/>
              <owl:Class rdf:ID="MatrixValue"/>
            </owl:unionOf>
          </owl:Class>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="&tensor_quantity;#isIndexOf"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <owl:Class rdf:about="&tensor_quantity;#MatrixQuantity"/>
              <owl:Class rdf:about="&tensor_quantity;#MatrixValue"/>
            </owl:unionOf>
          </owl:Class>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Index"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="&tensor_quantity;#determinesPositionOf"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <owl:Class rdf:about="&tensor_quantity;#ColumnVectorQuantity"/>
              <owl:Class rdf:about="&tensor_quantity;#ColumnVectorValue"/>
            </owl:unionOf>
          </owl:Class>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="&tensor_quantity;#MatrixQuantity">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasElement"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:about="&tensor_quantity;#ColumnVectorQuantity"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:datatype="&xsd;#int"
        >2</owl:hasValue>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="hasTensorOrder"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:ID="VectorQuantity"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="TensorQuantity"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:InverseFunctionalProperty rdf:ID="isOrderedBy"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="&tensor_quantity;#ColumnIndex"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&system;#hasValue"/>
        <owl:allValuesFrom>
          <owl:Class rdf:about="&tensor_quantity;#MatrixValue"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:InverseFunctionalProperty rdf:about="&tensor_quantity;#isOrderedBy"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="&tensor_quantity;#ColumnIndex"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="&tensor_quantity;#hasElement"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:about="&tensor_quantity;#ColumnVectorQuantity"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="&xsd;#string"
    >A MatrixQuantity is a second order TensorQuantity.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="VectorElement">
    <rdfs:comment rdf:datatype="&xsd;#string"
    >A VectorElement is a ScalarQuantity that is an element of a VectorQuantity</rdfs:comment>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:onProperty>
              <owl:ObjectProperty rdf:ID="isElementOf"/>
            </owl:onProperty>
            <owl:someValuesFrom>
              <owl:Class rdf:about="&tensor_quantity;#VectorQuantity"/>
            </owl:someValuesFrom>
          </owl:Restriction>
          <rdf:Description rdf:about="&system;#ScalarQuantity">
            <owl:disjointWith>
              <owl:Class rdf:about="&tensor_quantity;#TensorQuantity"/>
            </owl:disjointWith>
          </rdf:Description>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:ID="hasOrientation"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="&coordinate_system;#CoordinateSystemAxis"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="&xsd;#int"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="&tensor_quantity;#isElementOf"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:about="&tensor_quantity;#VectorQuantity"/>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="&tensor_quantity;#isElementOf"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="&tensor_quantity;#hasOrientation"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="&xsd;#int"
        >1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:InverseFunctionalProperty rdf:ID="hasIndex"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:about="&tensor_quantity;#Index"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:InverseFunctionalProperty rdf:about="&tensor_quantity;#hasIndex"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="&xsd;#int"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:InverseFunctionalProperty rdf:about="&tensor_quantity;#hasIndex"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:about="&tensor_quantity;#Index"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:ID="VectorElementValue"/>
        </owl:allValuesFrom>
        <owl:onProperty rdf:resource="&system;#hasValue"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="&tensor_quantity;#RowIndex">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <owl:Class rdf:about="&tensor_quantity;#VectorElement"/>
              <owl:Class rdf:about="&tensor_quantity;#VectorElementValue"/>
            </owl:unionOf>
          </owl:Class>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="&tensor_quantity;#determinesPositionOf"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="&tensor_quantity;#isIndexOf"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <owl:Class rdf:about="&tensor_quantity;#ColumnVectorQuantity"/>
              <owl:Class rdf:about="&tensor_quantity;#ColumnVectorValue"/>
            </owl:unionOf>
          </owl:Class>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="&tensor_quantity;#isIndexOf"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <owl:Class rdf:about="&tensor_quantity;#ColumnVectorQuantity"/>
              <owl:Class rdf:about="&tensor_quantity;#ColumnVectorValue"/>
            </owl:unionOf>
          </owl:Class>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="&tensor_quantity;#Index"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <owl:Class rdf:about="&tensor_quantity;#VectorElement"/>
              <owl:Class rdf:about="&tensor_quantity;#VectorElementValue"/>
            </owl:unionOf>
          </owl:Class>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="&tensor_quantity;#determinesPositionOf"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="&xsd;#string"
    >A RowIndex denotes the position of a scalar element within a column vector.</rdfs:comment>
    <owl:disjointWith rdf:resource="&tensor_quantity;#ColumnIndex"/>
  </owl:Class>
  <owl:Class rdf:about="&tensor_quantity;#VectorQuantity">
    <owl:disjointWith rdf:resource="&tensor_quantity;#MatrixQuantity"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:datatype="&xsd;#int"
        >1</owl:hasValue>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="&tensor_quantity;#hasTensorOrder"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:ID="VectorValue"/>
        </owl:allValuesFrom>
        <owl:onProperty rdf:resource="&system;#hasValue"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="&tensor_quantity;#TensorQuantity"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="&tensor_quantity;#hasElement"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="&system;#ScalarQuantity"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="&tensor_quantity;#hasElement"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="&tensor_quantity;#VectorElement"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="&xsd;#string"
    >A VectorQuantity is a first order TensorQuantity.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="&tensor_quantity;#VectorValue">
    <rdfs:comment rdf:datatype="&xsd;#string"
    >A VectorValue is a first order TensorValue.</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:about="&tensor_quantity;#MatrixValue"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&system;#isValueOf"/>
        <owl:allValuesFrom rdf:resource="&tensor_quantity;#VectorQuantity"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="&tensor_quantity;#VectorElementValue"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="&tensor_quantity;#hasElement"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="TensorValue"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <rdf:Description rdf:about="&system;#ScalarValue">
            <owl:disjointWith>
              <owl:Class rdf:about="&tensor_quantity;#TensorValue"/>
            </owl:disjointWith>
          </rdf:Description>
        </owl:allValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="&tensor_quantity;#hasElement"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="&tensor_quantity;#ColumnVectorQuantity">
    <rdfs:comment rdf:datatype="&xsd;#string"
    >A ColumnVectorQuantity represents a column vector of a MatrixQuantity.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="&xsd;#int"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="&tensor_quantity;#isElementOf"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="&tensor_quantity;#RowIndex"/>
        <owl:onProperty>
          <owl:InverseFunctionalProperty rdf:about="&tensor_quantity;#isOrderedBy"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:InverseFunctionalProperty rdf:about="&tensor_quantity;#isOrderedBy"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="&tensor_quantity;#RowIndex"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="&tensor_quantity;#isElementOf"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="&tensor_quantity;#MatrixQuantity"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:InverseFunctionalProperty rdf:about="&tensor_quantity;#hasIndex"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="&tensor_quantity;#ColumnIndex"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="&tensor_quantity;#isElementOf"/>
            </owl:onProperty>
            <owl:someValuesFrom rdf:resource="&tensor_quantity;#MatrixQuantity"/>
          </owl:Restriction>
          <owl:Class rdf:about="&tensor_quantity;#VectorQuantity"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&system;#hasValue"/>
        <owl:allValuesFrom>
          <owl:Class rdf:about="&tensor_quantity;#ColumnVectorValue"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="&xsd;#int"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:InverseFunctionalProperty rdf:about="&tensor_quantity;#hasIndex"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="&tensor_quantity;#ColumnIndex"/>
        <owl:onProperty>
          <owl:InverseFunctionalProperty rdf:about="&tensor_quantity;#hasIndex"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="&tensor_quantity;#TensorValue">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="&system;#QuantitativeValue"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="&tensor_quantity;#hasElement"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith rdf:resource="&system;#ScalarValue"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="&tensor_quantity;#Index"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:InverseFunctionalProperty rdf:about="&tensor_quantity;#isOrderedBy"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom>
          <owl:Class rdf:about="&tensor_quantity;#TensorQuantity"/>
        </owl:allValuesFrom>
        <owl:onProperty rdf:resource="&system;#isValueOf"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="&xsd;#int"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="&tensor_quantity;#hasTensorOrder"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="&tensor_quantity;#hasElement"/>
